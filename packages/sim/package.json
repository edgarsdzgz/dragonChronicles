{
  "name": "@draconia/sim",
  "version": "0.0.0-phase0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npx tsc -b",
    "clean": "rimraf dist",
    "echo-ok": "echo ok",
    "test": "vitest run --config ../../configs/vitest/vitest.config.ts",
    "test:watch": "vitest --config ../../configs/vitest/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config ../../configs/vitest/vitest.config.ts"
  },
  "dependencies": {
    "@draconia/shared": "workspace:*",
    "@draconia/engine": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "rimraf": "^5.0.5"
  }
}
